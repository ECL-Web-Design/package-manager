{
  "name": "@ecl-design/auto-typer",
  "version": "1.0.0",
  "description": "Component that types out a list of words sequentially with animation.",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "set PACKAGE_NAME=auto-typer&& vite build --config ../../../../vite.config.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "author": "ECL Web Design",
  "license": "ISC",
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "vite-plugin-libcss": "^1.1.1",
    "@types/vue": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vue-tsc": "^1.8.15"
  }
}
